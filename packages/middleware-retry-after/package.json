{
	"name": "@qfetch/middleware-retry-after",
	"description": "Fetch middleware for RFC 9110-compliant retry-after handling.",
	"version": "0.1.0",
	"author": "vabatta",
	"bugs": "https://github.com/qfetch/qfetch/issues",
	"devDependencies": {
		"@qfetch/core": "workspace:",
		"tsdown": "catalog:",
		"typescript": "catalog:"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"homepage": "https://github.com/qfetch/qfetch/tree/main/packages/middleware-retry-after#readme",
	"keywords": [
		"retry",
		"retry-after",
		"rate-limiting",
		"429",
		"503",
		"rfc9110",
		"composable",
		"fetch",
		"http",
		"middleware",
		"typescript"
	],
	"license": "MIT",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/qfetch/qfetch.git",
		"directory": "packages/middleware-retry-after"
	},
	"scripts": {
		"build": "tsdown",
		"check-types": "tsc --noEmit",
		"style": "biome check .",
		"test": "node --test",
		"test:e2e": "node --test --test-name-pattern=\"/E2E/i\"",
		"test:unit": "node --test --test-skip-pattern=\"/E2E/i\""
	},
	"type": "module",
	"types": "./dist/index.d.ts"
}
