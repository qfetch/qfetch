{
	"compilerOptions": {
		"noEmit": true,
		"target": "esnext",
		"module": "nodenext",
		"lib": [
			"esnext",
			"dom",
			"webworker"
		],
		"strict": true,
		"exactOptionalPropertyTypes": true,
		"declaration": true,
		"isolatedDeclarations": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"erasableSyntaxOnly": true,
		"verbatimModuleSyntax": true,
		"rewriteRelativeImportExtensions": true,
		"forceConsistentCasingInFileNames": true
	}
}
